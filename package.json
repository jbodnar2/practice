{
  "name": "practice",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node ./serve.js & postcss css/main.css --output css/style.css --verbose --watch --map",
    "php:serve": "php -S localhost:3000 & postcss css/main.css --output css/style.css --verbose --watch --map",
    "prettier:write": "prettier --write ./",
    "prettier": "prettier --check ./",
    "postcss": "postcss css/main.css --output css/style.css --verbose",
    "postcss:watch": "postcss css/main.css --output css/style.css --verbose --watch --map"
  },
  "prettier": {
    "semi": true,
    "printWidth": 80,
    "bracketSpacing": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "@prettier/plugin-php": "^0.19.1",
    "autoprefixer": "^10.4.13",
    "live-server": "^1.2.2",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1"
  }
}
